{
  "name": "avatarka",
  "version": "1.0.0",
  "description": "SVG avatar generators with multiple themes",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": ["avatar", "svg", "generator", "identicon"],
  "license": "MIT",
  "author": "Andrey Akinshin",
  "homepage": "https://github.com/AndreyAkinshin/avatarka",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreyAkinshin/avatarka.git"
  },
  "bugs": {
    "url": "https://github.com/AndreyAkinshin/avatarka/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  }
}
