{
  "name": "avatarka-react",
  "version": "1.1.0",
  "description": "React components for avatarka SVG avatar generator",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": ["dist"],
  "sideEffects": ["./dist/styles.css"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "avatarka": "workspace:^"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": ["avatar", "svg", "generator", "react", "component"],
  "license": "MIT",
  "author": "Andrey Akinshin",
  "homepage": "https://github.com/AndreyAkinshin/avatarka",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreyAkinshin/avatarka.git"
  },
  "bugs": {
    "url": "https://github.com/AndreyAkinshin/avatarka/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  }
}
